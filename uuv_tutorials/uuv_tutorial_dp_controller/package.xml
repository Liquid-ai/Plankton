<?xml version='1.0' encoding='utf-8'?>
<package format="2">
  <name>uuv_tutorial_dp_controller</name>
  <version>0.6.13</version>
  <description>
    Tutorial that shows how to inherit the controller interface modules from the uuv_trajectory_control package to
    develop a custom controlller.
  </description>

  <maintainer email="luizricardo.douat@de.bosch.com">Luiz Ricardo Douat</maintainer>
  <maintainer email="musa.marcusso@de.bosch.com">Musa Morena Marcusso Manhaes</maintainer>
  <maintainer email="sebastian.scherer2@de.bosch.com">Sebastian Scherer</maintainer>

  <license>Apache-2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <exec_depend>uuv_trajectory_control</exec_depend>
  <exec_depend>uuv_control_utils</exec_depend>
  <exec_depend>uuv_descriptions</exec_depend>
  <exec_depend>uuv_thruster_manager</exec_depend>
  <exec_depend>uuv_gazebo</exec_depend>

  <export>
  <build_type>ament_cmake</build_type>
  </export>
    </package>
